<?xml version="1.0" encoding="utf-8"?><!--<bitmap--><!--xmlns:android="http://schemas.android.com/apk/res/android"--><!--android:gravity="center"--><!--android:src="@mipmap/white_cell"></bitmap>-->
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <!--COLOR GREEN #99cc00-->
    <item>
        <shape>
            <corners android:radius="15dp" />
            <solid android:color="#3399cc00" />
            <padding
                android:bottom="3dp"
                android:left="3dp"
                android:right="3dp"
                android:top="3dp" />
        </shape>
    </item>

    <item>
        <shape>
            <corners android:radius="15dp" />
            <solid android:color="#6699cc00" />

            <padding
                android:bottom="2dp"
                android:left="2dp"
                android:right="2dp"
                android:top="2dp" />
        </shape>
    </item>

    <item>
        <shape>
            <corners android:radius="15dp" />
            <solid android:color="#9999cc00" />
            <padding
                android:bottom="1dp"
                android:left="1dp"
                android:right="1dp"
                android:top="1dp" />
        </shape>
    </item>

    <item>
        <bitmap
            android:gravity="center"
            android:src="@mipmap/white_cell" />
    </item>
</layer-list>

